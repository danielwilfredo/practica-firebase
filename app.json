{
  "expo": {
    "name": "firebase-app",
    "slug": "firebase-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pokebal_favicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/practica-firebase.png",
      "resizeMode": "contain",
      "backgroundColor": "#1680bb"
    },
    "ios": {
      "supportsTablet": true
    },
